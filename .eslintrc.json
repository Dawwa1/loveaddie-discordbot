{
	"extends": "eslint:recommended",
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2019
	},
	"rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "prefer-const": "error",
		"object-curly-spacing": ["error", "always"],
        "no-var": "error"

	}
}